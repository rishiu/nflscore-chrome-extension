{
	"manifest_version": 2,
	"name": "NFL scores",
	"description": "Shows current or recent NFL Scores",
	"version": "0.1",
	"browser_action": {
		"default_icon": "football-512.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"http://www.nfl.com/liveupdate/scorestrip/ss.xml",
		"http://www.nfl.com/liveupdate/scorestrip/postseason/ss.xml"
	]
	
}